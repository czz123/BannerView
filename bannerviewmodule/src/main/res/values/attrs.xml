<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BannerView">
        <attr name="indicatorwidth" format="dimension|reference"/>
        <attr name="indicatorheight" format="dimension|reference"/>
        <attr name="indicatormargin" format="dimension|reference"/>
        <attr name="indicatordrawable" format="reference"/>
        <attr name="delaytime" format="integer"/>
        <attr name="isautoplay" format="boolean"/>
        <attr name="speed" format="float"/>
        <attr name="indicatorgravity">
          <enum name="left" value="5"/>
          <enum name="center" value="6"/>
          <enum name="right" value="7"/>
        </attr>
    </declare-styleable>
</resources>